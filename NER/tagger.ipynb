    "\n",
    "        return train, test\n",
    "    \n",
    "    else:\n",
    "        size = df1.shape[0]\n",
    "        train = df1.loc[:(int)(size*train_size)]\n",
    "        test = df1.loc[(int)(size*train_size):]\n",
    "        test = test.reset_index(drop=True)\n",
    "        train = train.sample(len(train)).reset_index(drop=True)\n",
    "        test = test.sample(len(test)).reset_index(drop=True)\n",
    "        \n",
    "        return train, test"
